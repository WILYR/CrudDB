create table "Accounts" (
  "id" bigint generated by default as identity not null,
  "login" varchar(255) not null,
  "password" varchar(255) not null,
  "accountstatus" varchar(255) not null,
  primary key ("id")
);
create table "skills" (
  "id" bigint generated by default as identity not null,
  "name" varchar(255) not null,
  primary key ("id")
);
create table "accountstatus" (
  "status" varchar(255) not null,
  primary key ("status")
);
create table "developers" (
  "accountid" bigint not null,
  "id" bigint generated by default as identity not null,
  unique ("id"),
  primary key ("id")
);
create table "developer_skills" (
  "developer_id" bigint not null,
  "skill_id" bigint not null
);
alter table "Accounts"
  add constraint "Accounts_fk0"
    foreign key ("accountstatus")
    references "accountstatus" ("status");
alter table "developers"
  add constraint "developers_fk0"
    foreign key ("accountid")
    references "Accounts" ("id");
alter table "developer_skills"
  add constraint "developer_skills_fk0"
    foreign key ("developer_id")
    references "developers" ("id");
alter table "developer_skills"
  add constraint "developer_skills_fk1"
    foreign key ("skill_id")
    references "skills" ("id");
insert into accountstatus
values ('DELETED');
insert into accountstatus
values ('BANNED');
insert into accountstatus
values ('ACTIVE');